config EXAMPLES_CALCULATOR_CLIENT
	bool "Calculator client example"
	default n
	---help---
		Enable the Calculator client example

config USER_ENTRYPOINT
	string
	default "calculator_client_main" if ENTRY_CALCULATOR_CLIENT
