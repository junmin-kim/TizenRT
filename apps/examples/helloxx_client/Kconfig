config EXAMPLES_HELLOXX_CLIENT
	bool "\"Hello, World!\" C++ client example"
	default n
	---help---
		Enable the \"Hello, World!\" C++ client example

config USER_ENTRYPOINT
	string
	default "helloxx_client_main" if ENTRY_HELLOXX_CLIENT
